{
  "name": "taxjar/taxjar-php",
  "description": "Sales Tax API Client for PHP 5.5+",
  "keywords": [
    "taxjar",
    "sales tax",
    "api"
  ],
  "homepage": "https://www.taxjar.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "TaxJar"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.0.2|^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "TaxJar\\" : "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TaxJar\\Tests\\": "test/"
    }
  },
  "scripts": {
    "tests": "phpunit"
  }
}
